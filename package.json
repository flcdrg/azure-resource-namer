  "devDependencies": {
    "@aurelia/vite-plugin": "^2.0.0-beta.27",
    "@types/format-unicorn": "^1.1.5",
    "@types/jest": "^28.1.8",
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.26.0",
    "jest": "^28.1.3",
    "jest-cli": "^30.2.0",
    "jest-transform-stub": "^2.0.0",
    "promise-polyfill": "^8.3.0",
    "tree-kill": "^1.2.2",
